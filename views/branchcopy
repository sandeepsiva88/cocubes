    script.
      function college_select_all(){
      if (document.getElementById('all_colleges').checked) {
      document.getElementById('aec').checked="true";
      document.getElementById('acoe').checked="true";
      document.getElementById('acet').checked="true";
      }
      else {
      document.getElementById('aec').checked=false;
      document.getElementById('acoe').checked=false;
      document.getElementById('acet').checked=false;
      }
      }
      
      function college_selection_single() {
      //alert('aklfjaksjfdk');
      var i = document.getElementById('aec');
      var j = document.getElementById('acoe');
      var k = document.getElementById('acet');
      if (i.checked==true && j.checked==true && k.checked==true) {
      document.getElementById('all_colleges').checked=true;
      }
      else if (i.checked==false || j.checked==false || k.checked==false) {
      document.getElementById('all_colleges').checked=false;
      }
      }

      function courses_all_selection(){
      if (document.getElementById('all_courses').checked) {
      document.getElementById('civil').checked="true";
      document.getElementById('eee').checked="true";      
      document.getElementById('ece').checked="true";
      document.getElementById('cse').checked="true";
      document.getElementById('it').checked="true";
      document.getElementById('me').checked="true";
      document.getElementById('pt').checked="true";      
      document.getElementById('mca').checked="true";
      document.getElementById('agri').checked="true";
      document.getElementById('min').checked="true";
      }
      else {
      document.getElementById('civil').checked=false;
      document.getElementById('eee').checked=false;      
      document.getElementById('ece').checked=false;
      document.getElementById('cse').checked=false;
      document.getElementById('it').checked=false;
      document.getElementById('me').checked=false;
      document.getElementById('pt').checked=false;      
      document.getElementById('mca').checked=false;
      document.getElementById('agri').checked=false;
      document.getElementById('min').checked=false;
      }
      }
      
      function course_single_selection() {
      var a = document.getElementById('civil');
      var b = document.getElementById('eee');      
      var d = document.getElementById('ece');
      var e = document.getElementById('cse');
      var f = document.getElementById('it');
      var g = document.getElementById('me');
      var h = document.getElementById('pt');      
      var j = document.getElementById('mca');
      var k = document.getElementById('agri');
      var l = document.getElementById('min');
      if (a.checked==true && b.checked==true && d.checked==true && e.checked==true && f.checked==true && g.checked==true && h.checked==true && j.checked==true && k.checked==true && l.checked==true) {
      document.getElementById('all_courses').checked=true;
      }
      else if (a.checked==false || b.checked==false || d.checked==false || e.checked==false || f.checked==false || g.checked==false || h.checked==false || j.checked==false || k.checked==false || l.checked==false) {
      document.getElementById('all_courses').checked=false;
      }
      }